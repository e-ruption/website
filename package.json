{
  "name": "@e-ruption/website",
  "version": "0.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "bash ./.npm.scripts/clean.sh",
    "preps": "npm run preps:all",
    "preps:all": "bash ./.npm.scripts/dev/prepare.sh",
    "preps:g": "bash ./.npm.scripts/dev/prepare-g.sh",
    "preps:dev": "bash ./.npm.scripts/dev/prepare-dev.sh",
    "test": "npm run test:dev",
    "test:dev": "bash ./.npm.scripts/dev/test.sh",
    "test:staging": "bash ./.npm.scripts/staging/test.sh",
    "test:prod": "bash ./.npm.scripts/prod/test.sh",
    "build": "npm run build:dev",
    "build:dev": "bash ./.npm.scripts/dev/build.sh",
    "build:staging": "bash ./.npm.scripts/staging/build.sh",
    "build:prod": "bash ./.npm.scripts/prod/build.sh",
    "start": "npm run build:dev && bash ./.npm.scripts/dev/start.sh",
    "oci:build:dev": "cd ./.containers/dev && bash ./.build.sh",
    "oci:run:server:dev": "cd ./.containers/dev && bash ./.run.sh",
    "oci:tear:server:dev": "cd ./.containers/dev && bash ./.run.sh",
    "oci:run:hugo:dev": "unset export HUGO_BASE_URL && cd ./.containers/dev && ./.run.hugo.build.sh",
    "oci:run:hugo:prod": "export HUGO_BASE_URL='https://e-ruption.fr/' && cd ./.containers/dev && ./.run.hugo.build.sh",
    "dev": "npm run oci:build:dev && npm run oci:tear:server:dev && npm run oci:run:server:dev",
    "deploy:heroku:dev": "bash ./.npm.scripts/dev/deploy/heroku.sh",
    "deploy:surge:dev": "npm run clean && ./.npm.scripts/dev/deploy/surge.sh",
    "deploy:b4app:dev": "bash ./.npm.scripts/dev/deploy/back4app.sh",
    "deploy:heroku:staging": "bash ./.npm.scripts/staging/deploy/heroku.sh",
    "deploy:gh_pages:staging": "bash ./.npm.scripts/staging/deploy/gh_pages.sh",
    "deploy:heroku:prod": "bash ./.npm.scripts/prod/deploy/heroku.sh",
    "deploy:gh_pages:prod": "npm run clean && npm run build:prod && ./.npm.scripts/prod/deploy/gh_pages.sh",
    "ci:release:build:gh_pages": "bash ./.npm.scripts/dev/deploy/github-pages.sh",
    "ci:release:deploy:gh_pages": "bash ./.npm.scripts/dev/deploy/github-pages.sh",
    "ci:release:heroku": "bash ./.npm.scripts/release/heroku.sh",
    "spawn:project:clean": "echo \"cleaning hugo project\" && bash ./.npm.scripts/clean-hugo.sh",
    "spawn:project:gen": "echo \"spawn up new hugo project from Hugo theme\" && bash ./.npm.scripts/spawn.sh",
    "clean:hugo": "echo \"cleaning hugo public/ Folder \" && gulp clean:hugo",
    "parse-hugo-them-toml": "node .npm.scripts/toml-parser.js",
    "pr": "bash ./.npm.scripts/dev/inflate.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/e-ruption/website.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/e-ruption/website.git/issues"
  },
  "homepage": "https://github.com/e-ruption/website.git/#readme",
  "dependencies": {
    "bulma": "^0.9.3"
  },
  "devDependencies": {
    "@babel/core": "^7.19.1",
    "@babel/preset-env": "^7.19.1",
    "@babel/register": "^7.18.9",
    "browser-sync": "^2.27.10",
    "concat-stream": "^2.0.0",
    "dotenv": "^16.0.2",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-beautify": "^3.0.0",
    "gulp-debug": "^4.0.0",
    "gulp-dest-clean": "^0.5.0",
    "gulp-imagemin": "^8.0.0",
    "gulp-minify": "^3.1.0",
    "gulp-newer": "^1.4.0",
    "gulp-purgecss": "^4.1.3",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^5.1.0",
    "gulp-seo": "^1.0.2",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-uglify": "^3.0.2",
    "gulp-util": "^3.0.8",
    "imagemin-pngquant": "^9.0.2",
    "node-fetch": "^2.6.7",
    "node-sass": "^7.0.3",
    "readable-stream": "^3.6.0",
    "toml": "^3.0.0"
  }
}
